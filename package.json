{
  "name": "reqmarkup",
  "displayName": "ReqMarkup",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
  "onCommand:extension.addAnnotation",
  "onStartupFinished"
],
  "main": "./out/extension.js",
"contributes": {
  "commands": [
    {
      "command": "extension.addAnnotation",
      "title": "Add Requirement Annotation"
    },
    {
      "command": "extension.deleteAnnotation",
      "title": "Delete Requirement Annotation"
    },
    {
      "command": "extension.modifyAnnotation",
      "title": "Modify Requirement Annotation"
    }
  ],
  "menus": {
    "editor/context": [
      {
        "command": "extension.addAnnotation",
        "when": "editorHasSelection",
        "group": "navigation"
      },
      {
        "command": "extension.modifyAnnotation",
        "when": "editorTextFocus",
        "group": "navigation"
      },
      {
        "command": "extension.deleteAnnotation",
        "when": "editorTextFocus",
        "group": "navigation"
      }
    ]
  }
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.93.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "typescript": "^5.5.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
